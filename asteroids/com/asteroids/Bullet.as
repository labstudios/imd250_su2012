package com.asteroids{			public class Bullet extends GameObject	{		private var bulletSpeed:Number = 15;		private var frames:Number = 24;		public function Bullet(x:Number, y:Number, r:Number)		{			this.x = x;			this.y = y;			this.rotation = r;			this.speed = this.bulletSpeed;		}				override public function run():void		{			super.run();			this.frames--;			if(this.frames <= 0)			{				Game.instance.removeBullet(this);			}		}	}}